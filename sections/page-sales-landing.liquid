
{% if section.settings.enable %}
  <div class="shopify-section signup_banner">
    <div class="container-bootstrap">
      <div class="row">
        <div class="col-sm-6">
          <div class="signup_banner_cont">
            {{ section.settings.signup_banner_cont }}
          </div>
        </div>
        <div class="col-sm-6">
          <div class="sign_banner_right">
            <img src="https://cdn.shopify.com/s/files/1/0317/4635/9433/files/sale-landing-side.png?v=1674117798">
          </div>
        </div>
      </div>
    </div>
  </div>
  <div id="sign_two_blk">
    <div class="container-bootstrap">
      <div class="row">
        <div class="col-sm-12 are-you">
          {{ section.settings.are_you_sec }}
        </div>
      </div>
      <div class="row split">
        <div class="col-sm-6 clearfix">
          <div class="become_sec clearfix">
            <div class="become_inner">
              {{ section.settings.become_sec }}
              <div class="modal fade popup_content" id="membership_popup" role="dialog">
                <div class="modal-dialog">
                  <!-- Modal content -->
                  <div class="modal-content">
                    <div class="modal-header">
                      <button type="button" class="close" data-dismiss="modal">&times;</button>
                      {{ section.settings.member_popup_title }}
                    </div>
                    <div class="modal-body">
                      {% unless customer %}
                        <div class="login-page" style="display: none;">
                          {% comment %}
                            If a user has successfully requested a new password, the form.posted_successfully? variable
                            is set to true within the 'recover_customer_password' form only. Within that form you'll see
                            us assign a variable 'reset_success' to true, which is used in JS to show our success message.
                          {% endcomment %}

                          <div class="note form-success" id="resetSuccess" style="display:none;">
                            We've sent you an email with a link to update your password.
                          </div>

                          {% comment %}
                            This form must use 'customer_login'
                          {% endcomment %}
                          {% form 'customer_login' %}
                            <input type="hidden" name="checkout_url" value="/pages/signup-landing">
                            <header class="page-header">
                              <h1
                                {% if settings.enable_multilang %}
                                  data-translate="customer.login.title"
                                {% endif %}
                              >
                                Login
                              </h1>
                            </header>
                            <h6>Enter Your login information.</h6>
                            <p>
                              If you are not an existing customer, please
                              <a href="javaScript:void(0)" class="create_acc_btn">Create an account</a>
                            </p>
                            <div class="custom_acct_out">
                              {{ form.errors | default_errors }}
                              <label for="customer_email"
                                ><span
                                  {% if settings.enable_multilang %}
                                    data-translate="customer.login.email"
                                  {% endif %}
                                >
                                 Email</span
                                ><em>*</em></label
                              >
                              <input
                                type="email"
                                value=""
                                name="customer[email]"
                                id="customer_email"
                                {% if form.errors contains 'email' %}
                                  class="error"
                                {% endif %}
                              >

                              {% if form.password_needed %}
                                <label for="customer_password"
                                  ><span
                                    {% if settings.enable_multilang %}
                                      data-translate="customer.login.password"
                                    {% endif %}
                                  >
                                    Password</span
                                  ><em>*</em></label
                                >
                                <input
                                  type="password"
                                  value=""
                                  name="customer[password]"
                                  id="customer_password"
                                  {% if form.errors contains 'password' %}
                                    class="error"
                                  {% endif %}
                                >
                              {% endif %}

                              <div class="action-btn">
                                <p>
                                  <input
                                    type="submit"
                                    {% if settings.enable_multilang %}
                                      data-translate="customer.login.login"
                                    {% endif %}
                                    class="btn"
                                    value="Login"
                                  >
                                </p>
                                <p class="last">
                                  <a
                                    href="#"
                                    onclick="showRecoverPasswordForm();return false;"
                                    {% if settings.enable_multilang %}
                                      data-translate="customer.login.forgot_password"
                                    {% endif %}
                                  >
                                    Forgot Password
                                  </a>
                                  <!--
                                    <span>{{ 'cart.label.or' | t }}</span> <a href="{{ shop.url }}" {% if settings.enable_multilang %}data-translate="customer.login.cancel"{%endif%}>{{ 'customer.login.cancel' | t }}</a>
                                  -->
                                </p>
                              </div>
                            </div>
                          {% endform %}

                          {% comment %}
                            This page uses JavaScript to show/hide the recover password form
                          {% endcomment %}
                          <div id="recover_password" style="display: none;">
                            <header class="page-header">
                              <h2
                                {% if settings.enable_multilang %}
                                  data-translate="customer.recover_password.title"
                                {% endif %}
                              >
                                {{ 'customer.recover_password.title' | t }}
                              </h2>
                            </header>
                            <p
                              {% if settings.enable_multilang %}
                                data-translate="customer.recover_password.subtext"
                              {% endif %}
                            >
                              {{ 'customer.recover_password.subtext' | t }}
                            </p>

                            {% comment %}
                              This form must use 'recover_customer_password'
                            {% endcomment %}
                            {% form 'recover_customer_password' %}
                              {{ form.errors | default_errors }}
                              {% if form.posted_successfully? %}
                                {% assign reset_success = true %}
                              {% endif %}

                              {% comment %}
                                form.posted_successfully? is true only inside this form, but we want
                                to show a success message outside of it. To do so, set the variable
                                reset_success to true, then use JS to show our success message above.
                              {% endcomment %}
                              {% if form.posted_successfully? %}
                                {% assign resetPassword = true %}
                              {% endif %}

                              <label
                                for="recover-email"
                                {% if settings.enable_multilang %}
                                  data-translate="customer.recover_password.email"
                                {% endif %}
                              >
                                {{- 'customer.recover_password.email' | t -}}
                              </label>
                              <input
                                type="email"
                                value=""
                                name="email"
                                id="recover-email"
                                {% if settings.enable_multilang %}
                                  data-translate="customer.recover_password.email" translate-item="placeholder"
                                {% endif %}
                                placeholder="{{ 'customer.recover_password.email' | t }}"
                              >

                              <div class="action-btn">
                                <p>
                                  <input
                                    type="submit"
                                    class="btn"
                                    {% if settings.enable_multilang %}
                                      data-translate="customer.recover_password.submit"
                                    {% endif %}
                                    value="{{ 'customer.recover_password.submit' | t }}"
                                  >
                                </p>
                                <span
                                  {% if settings.enable_multilang %}
                                    data-translate="cart.label.or"
                                  {% endif %}
                                >
                                  {{- 'cart.label.or' | t -}}</span
                                ><a
                                  href="#"
                                  onclick="hideRecoverPasswordForm();return false;"
                                  {% if settings.enable_multilang %}
                                    data-translate="customer.recover_password.cancel"
                                  {% endif %}
                                >
                                  {{- 'customer.recover_password.cancel' | t -}}
                                </a>
                              </div>
                            {% endform %}

                            {% comment %}
                              If accounts are set as optional, the following will be shown as an option
                              during checkout, not on the default /login page.
                            {% endcomment %}

                            {% if shop.checkout.guest_login %}
                              <hr>
                              <h2
                                {% if settings.enable_multilang %}
                                  data-translate="customer.login.guest_title"
                                {% endif %}
                              >
                                {{ 'customer.login.guest_title' | t }}
                              </h2>
                              {% form 'guest_login' %}
                                <input
                                  type="submit"
                                  {% if settings.enable_multilang %}
                                    data-translate="customer.login.guest_continue"
                                  {% endif %}
                                  class="btn"
                                  value="{{ 'customer.login.guest_continue' | t }}"
                                >
                              {% endform %}
                            {% endif %}
                          </div>
                        </div>

                        <div class="create-an-account">
                          <header class="page-header">
                            <h4
                              {% if settings.enable_multilang %}
                                data-translate="customer.register.title"
                              {% endif %}
                            >
                              Create Account
                            </h4>
                          </header>
                          <h6>Fill in the fields below to create an account.</h6>
                          <p>
                            If you already have an account, please
                            <a href="javascript:void(0)" class="login_btn">Login</a>.
                          </p>
                          {% comment %}
                            {% if settings.enable_multilang %}
                            <p class="title lang1">{{ settings.register_text_desc | split: '|' | first }}</p>
                            <p class="title lang2">{{ settings.register_text_desc | split: '|' | last }}</p>
                            {% else %}
                            <p class="title">{{ settings.register_text_desc | split: '|' | first }}</p>
                            {% endif %}
                          {% endcomment %}
                          {% comment %}
                            This form must use 'create_customer'
                          {% endcomment %}
                          {% form 'create_customer' %}
                            <input type="hidden" name="checkout_url" value="/pages/signup-landing">
                            {% include 'form-errors-custom' %}
                            <div id="first_name">
                              <label for="first_name"
                                ><span
                                  {% if settings.enable_multilang %}
                                    data-translate="customer.register.first_name"
                                  {% endif %}
                                >
                                  First Name
                                </span></label
                              >
                              <input
                                type="text"
                                value=""
                                name="customer[first_name]"
                                id="first_name"
                                {% if form.errors contains 'first_name' %}
                                  class="error"
                                {% endif %}
                              >
                            </div>

                            <div id="last_name">
                              <label for="last_name"
                                ><span
                                  {% if settings.enable_multilang %}
                                    data-translate="customer.register.last_name"
                                  {% endif %}
                                >
                                  Last Name
                                </span></label
                              >
                              <input
                                type="text"
                                value=""
                                name="customer[last_name]"
                                id="last_name"
                                {% if form.errors contains 'last_name' %}
                                  class="error"
                                {% endif %}
                              >
                            </div>

                            <div id="email">
                              <label for="email"
                                ><span
                                  {% if settings.enable_multilang %}
                                    data-translate="customer.register.email"
                                  {% endif %}
                                >
                                 Email*</span
                                ><em>*</em></label
                              >
                              <input
                                type="email"
                                value=""
                                name="customer[email]"
                                id="email"
                                {% if form.errors contains 'email' %}
                                  class="error"
                                {% endif %}
                              >
                            </div>

                            <div id="create_password">
                              <label for="password"
                                ><span
                                  {% if settings.enable_multilang %}
                                    data-translate="customer.register.password"
                                  {% endif %}
                                >
                                  Password*</span
                                ><em>*</em></label
                              >
                              <input
                                type="password"
                                value=""
                                name="customer[password]"
                                id="create_password"
                                {% if form.errors contains 'password' %}
                                  class="error"
                                {% endif %}
                              >
                            </div>

                            <div class="action-btn">
                              <p>
                                <input
                                  type="submit"
                                  {% if settings.enable_multilang %}
                                    data-translate="customer.register.submit"
                                  {% endif %}
                                  value="Create an account"
                                  class="btn"
                                >
                              </p>
                            </div>
                          {% endform %}
                        </div>

                        <script>
                          /*
                            Show/hide the recover password form when requested.
                            I'd suggest using some CSS3 transitions to make this change look cool,
                            but this gets the job done for now.
                          */
                          function showRecoverPasswordForm() {
                            document.getElementById('recover_password').style.display = 'block';
                            document.getElementById('customer_login').style.display='none';
                          }

                          function hideRecoverPasswordForm() {
                            document.getElementById('recover_password').style.display = 'none';
                            document.getElementById('customer_login').style.display = 'block';
                          }

                          // Allow deep linking to the recover password form
                          if (window.location.hash == '#recover') { showRecoverPasswordForm() }

                          // reset_success is only true when the reset form is
                          {% if reset_success %}
                            document.getElementById('resetSuccess').style.display = 'block';
                          {% endif %}
                        </script>
                      {% endunless %}

                      <div class="prev_steps {% unless customer %}hide{% endunless %}">
                        {{ section.settings.member_popup_form }}
                      </div>
                      <div class="thankyou_content step3">
                        <div class="thanku_inner">
                          {{ section.settings.member_popup_thankyou }}
                        </div>
                        <div class="goto_btn step3">
                          {% if section.settings.goto_member_btn != blank %}
                            <a href="{{ section.settings.goto_member_link }}" class="goto">Go to Member Dashboard</a>
                          {% endif %}
                        </div>
                      </div>
                    </div>
                    <div class="modal-footer">
                      <div class="modal_ftr_right">
                        <button type="button" class="btn btn-default cancel_btn" data-dismiss="modal">Cancel</button>
                        {% if customer %}
                          <button type="button" class="btn btn-default next_step_btn step1">Next Step</button>
                          <button type="button" class="btn btn-default pay_btn step2">Pay</button>
                        {% endif %}
                      </div>
                      <div class="note_text step2">
                        {{ section.settings.member_popup_note }}
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-sm-6">
          <div class="are_you_sec clearfix">
            <div class="are_inner">
              <img src="https://cdn.shopify.com/s/files/1/0317/4635/9433/files/are-you-side.png?v=1674123317">
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
