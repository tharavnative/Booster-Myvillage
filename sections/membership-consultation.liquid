{% include 'bold-memberships-salt' %}
{% if section.settings.enable %}

<div class="membership-consultation">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <header class="page-header">
                    <div class="img_cart">
                        {% if section.settings.signup_banner != blank %} <img src="{{ section.settings.signup_banner | img_url :"master"}}" alt="{{ page.title | split: '|' | first }}" /> {% else %} {% capture current %}{% cycle 2%}{% endcapture %} {{
                        'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }} {% endif %}
                    </div>

                    {% if settings.enable_multilang %}
                    <h3>{{ page.title }}</h3>
                    {% else %}
                    <h3>{{ page.title | split: '|' | first }}</h3>
                    {% endif %}
                </header>
            </div>
        </div>
    </div> 
</div>

<div class="modal fade popup_content" id="membership_consultation_popup" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                {{ section.settings.member_popup_title }}
            </div>
            <div class="modal-body">
                {{ section.settings.member_popup_content }}
            </div> 
        </div>
    </div>
</div> 

<div class="modal fade popup_content" id="membership_book_75" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                {{ section.settings.member_popup_title }}
            </div>
            <div class="modal-body">
                {{ section.settings.member_initial_consultation }}
            </div> 
        </div>
    </div>
</div>

<div class="modal fade popup_content" id="membership_book_45" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
